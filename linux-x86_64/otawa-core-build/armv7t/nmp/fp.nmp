op VMOV_arm_reg(x: VMOV_arm_reg_list)
	syntax = x.syntax
	image = x.image
	action = {
		if ConditionPassed() then
			//CheckAdvSIMDOrVFPEnabled(TRUE, advsimd);
			if x.single_register then
				S[x.d] = S[x.m];
			else
				r_ = 0; loop;
			endif;
		endif;
	}
	loop = {
		if r_ <= x.regs-1 then
			D[x.d + r_] = D[x.m + r_];
			r_ = r_ + 1;
			loop;
		endif;
	}

