mode QuadReg(_p: card(1), _r: card(4)) = _p :: _r
	p = _p
	r = _r
	i = p :: r
	syntax = format("q%d", i)

mode DoubleReg(_p: card(1), _r: card(4)) = _p :: _r
	p = _p
	r = _r
	i = p :: r
	syntax = format("d%d", i)
	image = format("%4b", i)

mode SingleReg(_r: card(4), _p: card(1)) = _r :: _p
	r = _r
	p = _p
	i = r :: p
	syntax = format("s%d", i)


